<table style="width:100%">
	<thead>
		<tr>
			<th class="col-radio"></th>
			<th class="col-text"><%=t('contact.fields.type')%></th>
			<th class="col-text"><%=t('contact.fields.value')%></th>
	  		<%= render :partial => "shared/ui/date_th"%>			
			<th class="col-number"></th>
		</tr>
	</thead>
	<tfoot>
		<tr>
			<th class="list-paginate" colspan="6"> <%= will_paginate contacts, :param_name => param_name, :params => {:anchor => param_name} %> </th>
		</tr>
	</tfoot>
	<tbody>
		<% contacts.each do |contact|%>
		<tr>
			<td class="col-radio"></td>
			<td class="col-text"><%= contact.contact_type.name%></td>
			<td class="col-text"><%= contact.contact_value%></td>
	  		<%= render :partial => "shared/ui/date_td", :locals => {:object => contact}%>			
			<td class="col-number"> 
				<%=link_to 'Show', path_elements+[contact] %> 
			</td>
		</tr>
		<%end%>
		<% if contacts.empty? %>
		<tr>
			<td colspan="6">&nbsp;</td>
		</tr>
		<%end%>
	</tbody>
</table>
