<%= fields.fields_for :matter do |m_f| %>
	<%= m_f.fields_for :trademark do |t_f| %>
		<%= t_f.hidden_field :matter_id %>
		<ul>
			<li>
				<%= render :partial => "shared/matter/matter_number_field", :locals => {:fields 	=> fields}%>
			</li>
			<li>
				<%= render :partial => "shared/matter/description_field", :locals => {:fields 	=> fields}%>				
			</li>
			<li>
				<ul>
					<li>
						<h3>Customer information</h3>
					</li>
					<li>
						<%= render :partial => "shared/matter/agent_field", :locals => {:fields 	=> m_f}%>
					</li>
					<li>
						<%= render :partial => "shared/matter/applicant_field", :locals => {:fields 	=> m_f}%>
					</li>
				</ul>
			</li>
			<li>
				<ul>
					<li>
						<h3>Application details</h3>
					</li>
					<li>
						<%= t_f.label :mark_name %>
						<%= t_f.text_field :mark_name %>
					</li>
					<li>
						<%= t_f.label :appl_date %>
						<%= t_f.text_field :appl_date, :class => 'field-date' %>
					</li>
					<li>
						<%= t_f.label :appl_number %>
						<%= t_f.text_field :appl_number %>
					</li>
				</ul>
			</li>
			<li>
				<%= t_f.label :priority_date %>
				<%= t_f.text_field :priority_date %>
			</li>
			<li>
				<ul>
					<li>
						<h3>Trademark data</h3>
					</li>
					<li>
						<%= t_f.label :cfe_index%>
						<%= t_f.text_field :cfe_index %>
					</li>
					<li>
						<%= t_f.label :CTM_number %>
						<%= t_f.text_field :ctm_number %>
					</li>
					<li>
						<%= t_f.label :WIPO_number %>
						<%= t_f.text_field :wipo_number %>
					</li>
					<li>
						<%= t_f.label :reg_number %>
						<%= t_f.text_field :reg_number %>
					</li>
				</ul>
			</li>
			<li>
				<%= render :partial => "shared/matter/notes_field", :locals => {:fields 	=> fields}%>
			</li>
		</ul>
	<%end%>
<%end%> 