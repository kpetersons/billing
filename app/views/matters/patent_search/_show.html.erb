<ul class="fields-show">
	<li>
		<dl>
			<%= render :partial => "shared/matter/matter_number_show", :locals => {:matter 	=> matter}%>
		</dl>
	</li>
	<li>
		<ul>
			<li>
				<h3><%=t('header.customer_information')%></h3>
			</li>
			<li>
				<dl>
					<%= render :partial => "shared/matter/agent_show", :locals => {:matter 	=> matter}%>
					<%= render :partial => "shared/matter/applicant_show", :locals => {:matter 	=> matter}%>
				</dl>
			</li>
		</ul>
	</li>
	<li>
		<ul>
			<li>
				<h3><%=t('header.patent_search_details')%></h3>
			</li>
			<li>
				<dl>
					<dt>
						<label><%= t('matter.patent_search.fields.patent_eq_numbers')%></label>
					</dt>
					<dd>
						<%= matter.patent_search.patent_eq_numbers %>&nbsp;
					</dd>
					<dt>
						<label><%= t('matter.patent_search.fields.no_of_patents')%></label>
					</dt>
					<dd>
						<%= matter.patent_search.no_of_patents %>&nbsp;
					</dd>
					<dt>
						<label><%= t('matter.patent_search.fields.date_of_order')%></label>
					</dt>
					<dd>
						<%= matter.patent_search.date_of_order.to_s(:show) unless matter.patent_search.date_of_order.nil? %>&nbsp;
					</dd>
				</dl>
			</li>
		</ul>
	</li>
	<li>
		<dl>
			<%= render :partial => "shared/matter/notes_show", :locals => {:matter 	=> matter}%>
		</dl>
	</li>
</ul>